# A convenience wrapper
develop:
	@make develop -f ./themes/pxdocs-tooling/build/Makefile
update-theme:
	@make update-theme -f ./themes/pxdocs-tooling/build/Makefile
reset-theme:
	@make reset-theme -f ./themes/pxdocs-tooling/build/Makefile
check-links:
	@make check-links -f ./themes/pxdocs-tooling/build/Makefile
search-index:
	@make search-index -f ./themes/pxdocs-tooling/build/Makefile

